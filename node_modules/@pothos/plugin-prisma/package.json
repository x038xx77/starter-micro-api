{
  "name": "@pothos/plugin-prisma",
  "version": "3.35.8",
  "description": "A Pothos plugin for more efficient integration with prisma",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "module": "./esm/index.js",
  "exports": {
    "import": {
      "default": "./esm/index.js"
    },
    "require": {
      "types": "./dts/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayes/pothos.git"
  },
  "bin": {
    "prisma-pothos-types": "./bin/generator.js"
  },
  "author": "Michael Hayes",
  "license": "ISC",
  "keywords": [
    "pothos",
    "graphql",
    "schema",
    "typescript",
    "prisma",
    "db",
    "postgres",
    "mysql",
    "sql",
    "sqlite",
    "mongo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.5.0"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "peerDependencies": {
    "@pothos/core": "*",
    "@prisma/client": "*",
    "graphql": ">=15.1.0",
    "typescript": ">=4.7.2"
  },
  "devDependencies": {
    "@pothos/core": "3.22.9",
    "@pothos/plugin-complexity": "3.11.7",
    "@pothos/plugin-errors": "3.8.7",
    "@pothos/plugin-relay": "3.28.7",
    "@pothos/plugin-simple-objects": "3.6.7",
    "@pothos/test-utils": "1.4.7",
    "@prisma/client": "^4.5.0",
    "graphql": "16.6.0",
    "graphql-tag": "^2.12.6",
    "prisma": "^4.5.0"
  },
  "scripts": {
    "generate": "prisma generate",
    "type": "tsc --project tsconfig.type.json",
    "build": "pnpm build:clean && pnpm build:cjs && pnpm build:dts && pnpm build:esm",
    "build:clean": "git clean -dfX esm lib",
    "build:cjs": "swc src -d lib --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "cp -r dts/* esm/ && swc src -d esm --config-file ../../.swcrc -C module.type=es6 && pnpm esm:extensions",
    "build:dts": "tsc",
    "esm:extensions": "TS_NODE_PROJECT=../../tsconfig.json node -r @swc-node/register ../../scripts/esm-transformer.ts",
    "test": "pnpm vitest --run --segfault-retry=3"
  }
}